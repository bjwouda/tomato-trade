<div class="jumbotron">
  <div class="title-container">
    {{player-line player=player}}
  </div>
  <div class="row">
    <div class="col-sm-12">
      <h3 class="property-label">{{t 'results.player.averageUnitPrice'}}</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <p class="property-value"><span class="to-money" />{{digit-group-format averageUnitPrice}}</p>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      {{player-results-player-transactions player=player histories=histories buyers=buyers sellers=sellers}}
    </div>
  </div>
</div>

